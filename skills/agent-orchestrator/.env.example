# LLM Configuration
LLM_URL=https://openrouter.ai/api/v1/chat/completions
LLM_API_KEY=your-api-key-here
LLM_MODEL=openai/gpt-5.2-codex

# Optional: LLM timeout in seconds
LLM_TIMEOUT=60

# Workspace Configuration
# Base path for all OpenClaw operations.
# Runtime project_id is generated dynamically by orchestrator.run_workflow().
BASE_PATH=./workspace
PROJECT_ID=default_project

# OpenClaw Agent Command Runtime
OPENCLAW_API_BASE_URL=http://127.0.0.1:18789
OPENCLAW_API_KEY=your-openclaw-token-here
ORCH_WORKER_JOB_TIMEOUT_SECONDS=600

# Channel Notification (built-in notifier)
ORCH_MAIN_CHANNEL_ID=
ORCH_NOTIFY_WEBHOOK_URL=
ORCH_AGENT_CHANNELS=
ORCH_NOTIFY_TIMEOUT_SECONDS=10
ORCH_DISCORD_RETRY_MAX=1
ORCH_DISCORD_RETRY_DELAYS=3
ORCH_DISCORD_BOT_TOKEN=
ORCH_OPENCLAW_CONFIG_PATH=/home/ubuntu/.openclaw/openclaw.json
